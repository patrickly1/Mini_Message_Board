<html>
    <head>
        <link rel="stylesheet" href="./stylesnavbar.css">
    </head>
    <body>
        <nav>
            <ul>
                <% for (let i = 0; i < messages.length; i++) { %>
                <li>
                    <%= messages[i].user %>
                    <%= messages[i].text %>
                    <%= messages[i].added %>
                    <a href="/message/<%= i %>">Open Message</a>
                </li>
                <% } %>
            </ul>
        </nav>
    </body>
</html>
